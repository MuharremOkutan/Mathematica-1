Cell[TextData[{
  "We can now compute the vector ",
  Cell[BoxData[
      FormBox[
        StyleBox[\(\[Del]f\),
          FontWeight->"Bold"], TraditionalForm]]],
  "."
}], "Text"],

Cell[BoxData[
    \(gradf[x, y]\)], "Input"],

Cell[TextData[{
  "To compute a directional derivative, we first define a vector ",
  Cell[BoxData[
      \(TraditionalForm\`v\)],
    FontWeight->"Bold"],
  " which indicates the desired direction."
}], "Text"],

Cell[BoxData[
    \(v\  = \ {2, 3}\)], "Input"],

Cell[BoxData[
    RowBox[{
      StyleBox["We",
        FontFamily->"Times New Roman",
        FontVariations->{"CompatibilityType"->0}], 
      StyleBox[" ",
